- title "Coaches"

%table{:class => "players_team_list"}
  %th Mas Info
  %th Nombre Completo
  %th Equipo
  %th Email
  %th Opciones
  - for coach in @coaches
    %tr
      %td
        %span
          %small= link_to  image_tag('lupa.png'), coach_path(coach)
      %td= full_name(coach)
      %td= link_to coach.team.name, team_path(coach.team)
      %td= coach.email
      %td{:class => "last"}
        %span
          %small= link_to  image_tag('edit.png'), edit_coach_path(coach)
        

